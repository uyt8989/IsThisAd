{
    "name": "isThisAd",
    "description": "AD detector at Naver Blog",
    "version": "1.0",
    "manifest_version": 3,
    "host_permissions" : [
        "<all_urls>"
    ],
    "permissions": [
        "activeTab", 
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/new_logo_16.png",
            "32": "/images/new_logo_32.png",
            "48": "/images/new_logo_64.png",
            "128": "/images/new_logo_128.png"
        }
    },
    "icons": {
        "16": "/images/new_logo_16.png",
        "32": "/images/new_logo_32.png",
        "48": "/images/new_logo_64.png",
        "128": "/images/new_logo_128.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    }
}  